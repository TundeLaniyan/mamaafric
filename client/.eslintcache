ush({ ...addItem, quantity });\n    setBasket(current);\n  };\n\n  return (\n    <div>\n      <div className=\"history-box\">\n        <span className=\"history-text\">home /</span>\n        <span className=\"history-text\">food /</span>\n        <span className=\"history-text\">rice /</span>\n        <span className=\"history-text\">**** /</span>\n        <span className=\"history-text\">**** </span>\n      </div>\n      <div className=\"item\">\n        <div className=\"item__big-container\">\n          <div className=\"item__container-1\">\n            {infoLoading ? (\n              <ReactSpinner\n                type=\"Circles\"\n                color=\"#ffe7ba\"\n                height={\"22.5%\"}\n                width={\"100%\"}\n              />\n            ) : (\n              <img\n                src={`/img/${item.image}`}\n                alt=\"\"\n                className=\"item__img\"\n                onError={(e) => (e.target.src = \"/img/logo2.png\")}\n              />\n            )}\n\n            <div className=\"item__box\">\n              <div className=\"catergory__box\">\n                <span className=\"item__category\">Category /</span>\n                <span className=\"item__category-link\">Food</span>\n              </div>\n\n              <div className=\"item__name\">\n                {infoLoading ? (\n                  <ReactSpinner\n                    type=\"Circles\"\n                    color=\"#ffe7ba\"\n                    height={20}\n                    width={\"20%\"}\n                  />\n                ) : (\n                  item.name\n                )}\n              </div>\n\n              <div className=\"item__price\">\n                £\n                {infoLoading ? (\n                  <ReactSpinner\n                    type=\"Circles\"\n                    color=\"#ffe7ba\"\n                    height={20}\n                    width={\"20%\"}\n                  />\n                ) : (\n                  item.price\n                )}\n              </div>\n\n              <div className=\"item__description\">\n                {infoLoading ? (\n                  <ReactSpinner\n                    type=\"Circles\"\n                    color=\"#ffe7ba\"\n                    height={20}\n                    width={\"20%\"}\n                  />\n                ) : (\n                  item.description\n                )}\n              </div>\n\n              {infoLoading ? (\n                <ReactSpinner\n                  type=\"Circles\"\n                  color=\"#ffe7ba\"\n                  height={\"30%\"}\n                  width={\"100%\"}\n                />\n              ) : (\n                <>\n                  <input\n                    className=\"item__quantity-box\"\n                    type=\"number\"\n                    value={quantity}\n                    min={1}\n                    step={1}\n                    onChange={(e) => setQuantity(e.target.value * 1)}\n                  />\n                  <span className=\"item__quantity\">Quantity</span>\n                  <button className=\"item__btn\">\n                    <div className=\"item__btn-text\">BUY NOW</div>{\" \"}\n                  </button>\n                  <button className=\"item__btn\" onClick={handleAddBasket}>\n                    <div className=\"item__btn-text\">ADD TO CART</div>\n                  </button>\n                </>\n              )}\n            </div>\n          </div>\n\n          <div className=\"items__service\">\n            <div className=\"service\">\n              <div className=\"service__container\">\n                <div className=\"service__icon service__icon-1\"></div>\n                <div className=\"service__box\">\n                  <div className=\"service__title\">FREE SHIPPING</div>\n                  <div className=\"service__text\">On order over £100</div>\n                </div>\n              </div>\n\n              <div className=\"service__container\">\n                <div className=\"service__icon service__icon-2\"></div>\n                <div className=\"service__box\">\n                  <div className=\"service__title\">ALWAYS FRESH</div>\n                  <div className=\"service__text\">Product well packaged</div>\n                </div>\n              </div>\n              <div className=\"service__container\">\n                <div className=\"service__icon service__icon-3\"></div>\n                <div className=\"service__box\">\n                  <div className=\"service__title\">SUPERIOR QUALITY</div>\n                  <div className=\"service__text\">Quality products</div>\n                </div>\n              </div>\n              <div className=\"service__container\">\n                <div className=\"service__icon service__icon-4\"></div>\n                <div className=\"service__box\">\n                  <div className=\"service__title\">SUPPORT</div>\n                  <div className=\"service__text\">2/7 support servive</div>\n                </div>\n              </div>\n            </div>\n          </div>\n\n          <div className=\"feedback\">\n            <div className=\"feedback__header\">Feedback/Review</div>\n            <div className=\"feedback__text\">\n              Lorem ipsum dolor sit, amet consectetur adipisicing elit. Soluta\n              nostrum laborum beatae architecto, quaerat hic minus ut\n              perferendis, sit autem dicta, id est debitis consectetur accusamus\n              quia rerum molestiae porro. <b>more...</b>\n            </div>\n          </div>\n        </div>\n\n        <div className=\"item__container-2\">\n          <div className=\"item__container-2__header\">Shipping Info</div>\n          {/* <div className=\"item__container-2__text\">\n            Lorem ipsum dolor sit, amet consectetur adipisicing elit. Tenetur,\n            incidunt pariatur harum possimus exercitationem debitis eum ipsam\n            doloribus quam repellendus,{\" \"}\n          </div> */}\n          <div className=\"item__container-2__header\">Delivery</div>\n          <div className=\"item__container-2__text\">\n            Normally delivered between 2 days after confirmation of your order\n            date. Please check exact dates in the Checkout page.See more{\" \"}\n          </div>\n          <div className=\"item__container-2__header\">Return Policy</div>\n          <div className=\"item__container-2__text\">\n            Free return within 15 days.See more{\" \"}\n          </div>\n          <div className=\"item__container-2__header\">Warranty</div>\n          <div className=\"item__container-2__text\">-</div>\n        </div>\n      </div>\n      <section className=\"latest-products\">\n        <div className=\"latest-box\">\n          <h2 className=\"latest\">RELATED PRODUCTS</h2>\n          <Link to=\"/products\">\n            <h2 className=\"see-all\">see all</h2>\n          </Link>\n        </div>\n        <div className=\"products-slidder\">\n          {randomProducts.slice(0, 5).map((cur, i) =>\n            randomLoading ? (\n              <ReactSpinner\n                key={i}\n                type=\"Circles\"\n                color=\"#ffe7ba\"\n                height={80}\n                width={80}\n              />\n            ) : (\n              <div className=\"products-container\" key={i}>\n                <Link to={`item/${cur._id}`}>\n                  <img\n                    className=\"products-image\"\n                    src={`/img/${cur.image}`}\n                    onError={(e) => (e.target.src = \"/img/logo2.png\")}\n                    alt=\"\"\n                  />\n                  <h5 className=\"products-name\">{cur.name}</h5>\n                </Link>\n                <h6 className=\"products-price\">{cur.price}</h6>\n                <button className=\"add-basket-btn\">\n                  <div\n                    className=\"add-basket-text\"\n                    onClick={() => handleAddBasket(cur)}\n                  >\n                    ADD TO BASKET\n                  </div>\n                </button>\n              </div>\n            )\n          )}\n        </div>\n      </section>\n    </div>\n  );\n};\n\nexport default Item;\n","/Users/admin/Desktop/Full stack/mamaafric/client/src/pages/login/login.jsx",[],"/Users/admin/Desktop/Full stack/mamaafric/client/src/pages/database/database.jsx",["105","106"],"import React, { useEffect, useState } from \"react\";\nimport { addItem, updateItem } from \"../../services/adminService\";\nimport ReactSpinner from \"react-loader-spinner\";\nimport \"./database.scss\";\nimport { getProduct } from \"../../services/productService\";\n\nconst Database = ({ match }) => {\n  const id = match.params._id == 0 ? 0 : match.params._id;\n  const [pending, setPending] = useState(false);\n  const [status, setStatus] = useState(\"\");\n  const [product, setProduct] = useState({\n    name: \"\",\n    brand: \"\",\n    category: \"\",\n    type: \"\",\n    description: \"\",\n    price: 0,\n    image: {},\n  });\n\n  useEffect(() => {\n    const fetchData = async () => {\n      const product = await getProduct(id);\n\n      setProduct({\n        price: product.price,\n        name: product.name,\n        brand: product.brand,\n        category: product.category,\n        type: product.type,\n        description: product.description,\n        image: {},\n      });\n    };\n    id && fetchData();\n  }, []);\n\n  const handleOnSubmit = async (e) => {\n    e.preventDefault();\n    setStatus(\"\");\n    setPending(true);\n    const input = new FormData();\n    console.log(product);\n    Object.entries(product).forEach((cur) => {\n      if (cur[0] === \"image\" && !cur[1]) return;\n      input.append(cur[0], cur[1]);\n    });\n    const status = id ? await updateItem(id, input) : await addItem(input);\n    setStatus(status);\n    setPending(false);\n    if (status === \"success\") window.location = \"/admin-panel/items\";\n  };\n\n  const handleOnChange = ({ target }) => {\n    const { id, value, files } = target;\n    const current = { ...product };\n    current[id] = id === \"image\" ? files : value;\n    setProduct(current);\n  };\n\n  return (\n    <div className=\"database\">\n      <form onSubmit={handleOnSubmit}>\n        <h1>{id ? \"Update\" : \"Add\"} item</h1>\n        <div>\n          <label htmlFor=\"name\">Name</label>\n          <input\n            id=\"name\"\n            required\n            value={product.name}\n            onChange={handleOnChange}\n          />\n        </div>\n        <div>\n          <label htmlFor=\"brand\">Brand</label>\n          <input id=\"brand\" value={product.brand} onChange={handleOnChange} />\n        </div>\n        <div>\n          <label htmlFor=\"category\">Category</label>\n          <select\n            id=\"category\"\n            required\n            value={product.category}\n            onChange={handleOnChange}\n          >\n            <option></option>\n            {[\n              \"vegetables & ethnic foods\",\n              \"health & beauty\",\n              \"accessories & antiques\",\n              \"drinks & beverages\",\n            ].map((val) => (\n              <option key={val} value={val}>\n                {val}\n              </option>\n            ))}\n          </select>\n        </div>\n        <div>\n          <label htmlFor=\"type\">Type</label>\n          <input\n            id=\"type\"\n            required\n            value={product.type}\n            onChange={handleOnChange}\n          />\n        </div>\n        <div>\n          <label htmlFor=\"description\">Description</label>\n          <input\n            id=\"description\"\n            required\n            value={product.description}\n            onChange={handleOnChange}\n          />\n        </div>\n        <div>\n          <label htmlFor=\"price\">Price</label>\n          <input\n            type=\"number\"\n            id=\"price\"\n            step={0.01}\n            required\n            value={product.price}\n            onChange={handleOnChange}\n          />\n        </div>\n        <div>\n          <label htmlFor=\"image\">Image</label>\n          <input\n            type=\"file\"\n            id=\"image\"\n            name=\"image\"\n            accept=\"image/*\"\n            onChange={handleOnChange}\n          />\n        </div>\n        <div className=\"status\">\n          {pending ? (\n            <ReactSpinner type=\"Circles\" color=\"#e47b58\" height={25} />\n          ) : (\n            <div className={`status--${status}`}>{status}</div>\n          )}\n        </div>\n        <button className=\"login__submit\">{id ? \"Update\" : \"Submit\"}</button>\n      </form>\n    </div>\n  );\n};\n\nexport default Database;\n","/Users/admin/Desktop/Full stack/mamaafric/client/src/pages/checkout/checkout.jsx",[],"/Users/admin/Desktop/Full stack/mamaafric/client/src/services/emailServices.js",[],"/Users/admin/Desktop/Full stack/mamaafric/client/src/services/productService.js",[],"/Users/admin/Desktop/Full stack/mamaafric/client/src/component/shoppingCart/shoppingCart.jsx",[],"/Users/admin/Desktop/Full stack/mamaafric/client/src/component/product/product.jsx",[],"/Users/admin/Desktop/Full stack/mamaafric/client/src/services/httpService.js",["107","108"],"import axios from \"axios\";\nimport { toast } from \"react-toastify\";\naxios.defaults.withCredentials = true;\naxios.interceptors.response.use(null, (err) => {\n  if (err.response && err.response.status > 500) {\n    toast.error(\"An unexpected error occurrred.\");\n  }\n\n  return Promise.reject(err);\n});\n\nasync function postData(url = \"\", data = {}) {\n  const response = await fetch(url, {\n    method: \"POST\",\n    credentials: \"include\",\n    // mode: \"no-cors\",\n    headers: {\n      \"Content-Type\": \"application/json\",\n    },\n    body: JSON.stringify(data),\n  });\n  return response.json();\n}\n\nexp